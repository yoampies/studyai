{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler", /* Cambia de 'nodenext' a 'bundler' */
    "allowImportingTsExtensions": true,
    "noEmit": true, /* Vite se encarga de la compilaci√≥n */
    "jsx": "react-jsx",
    "strict": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    }
  },
  "include": ["src"]
}